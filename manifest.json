{
    "manifest_version": 3,
    "name": "LeetCode Dark Mode",
    "version": "1.0",
    "description": "Forces LeetCode into dark mode.",
    "host_permissions": ["https://leetcode.com/*"],
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  